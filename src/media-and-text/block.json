{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "miscellaneous-gutenberg-blocks/media-and-text",
	"version": "0.1.0",
	"title": "Media & Text",
	"category": "miscellaneous-gutenberg-blocks",
	"description": "",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true
		},
		"background": {
			"backgroundSize": true
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		}
	},
	"attributes": {
		"imageId": {
			"type": "number",
			"default": 0
		},
		"imageUrl": {
			"type": "string",
			"default": ""
		},
		"imageName": {
			"type": "string",
			"default": ""
		},
		"reversed": {
			"type": "boolean",
			"default": false
		},
		"tablet_reversed": {
			"type": "boolean",
			"default": false
		},
		"mobile_reversed": {
			"type": "boolean",
			"default": false
		},
		"gap": {
			"type": "number",
			"default": 15
		},
		"tablet_gap": {
			"type": "number",
			"default": 15
		},
		"mobile_gap": {
			"type": "number",
			"default": 15
		},
		"disable_css": {
			"type": "boolean",
			"default": false
		},
		"show_category": {
			"type": "boolean",
			"default": false
		},
		"show_search_icon": {
			"type": "boolean",
			"default": true
		},
		"button_inside": {
			"type": "boolean",
			"default": true
		},
		"search_placeholder": {
			"type": "string",
			"default": ""
		},
		"button_text": {
			"type": "string",
			"default": ""
		},
		"category_text": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "miscellaneous-gutenberg-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}