{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "miscellaneous-gutenberg-blocks/media-and-text",
	"version": "0.1.0",
	"title": "Media & Text",
	"category": "miscellaneous-gutenberg-blocks",
	"description": "",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true
		},
		"background": {
			"backgroundSize": true,
			"backgroundImage": true
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		}
	},
	"attributes": {
		"imageId": {
			"type": "number",
			"default": 0
		},
		"imageUrl": {
			"type": "string",
			"default": ""
		},
		"imageName": {
			"type": "string",
			"default": ""
		},
		"reversed": {
			"type": "boolean",
			"default": false
		},
		"tabletReversed": {
			"type": "boolean",
			"default": false
		},
		"mobileReversed": {
			"type": "boolean",
			"default": false
		},
		"stacked": {
			"type": "boolean",
			"default": false
		},
		"tabletStacked": {
			"type": "boolean",
			"default": false
		},
		"mobileStacked": {
			"type": "boolean",
			"default": false
		},
		"gap": {
			"type": "number",
			"default": 15
		},
		"tabletGap": {
			"type": "number",
			"default": 15
		},
		"mobileGap": {
			"type": "number",
			"default": 15
		}
	},
	"textdomain": "miscellaneous-gutenberg-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}